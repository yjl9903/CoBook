{
  "compilerOptions": {
    "target": "es2018",
    "module": "esnext",
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["esnext", "DOM"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true
  }
}